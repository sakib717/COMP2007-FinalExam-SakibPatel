@using COMP2006_S2016_FinalExamV2.Models
@model IEnumerable<Genre>


<ul id="categories">
    @foreach (var genre in Model)
    {
        <li>
            @Html.ActionLink(genre.Name,
                "Browse", "Store",
                new { Genre = genre.Name }, null)
    </li>
    }
</ul>

